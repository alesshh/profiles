<h2><%= t('.sign_up') %></h2>

<%= simple_form_for resource, url: registration_path(resource_name) do |f| %>
  <%= f.error_notification %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <hr>
  <%= f.association :state %>
  <%= f.input :title_position %>
  <%= f.input :age %>
  <%= f.button :submit, t('.submit') %>
<% end %>

<%= render "devise/shared/links" %>
