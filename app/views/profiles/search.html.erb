<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%= Profile.model_name.human(count: 2) %></h3>
  </div>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th><%= Profile.human_attribute_name(:email) %></th>
          <th><%= Profile.human_attribute_name(:state) %></th>
          <th><%= Profile.human_attribute_name(:job_title) %></th>
          <th><%= Profile.human_attribute_name(:age) %></th>
        </tr>
      </thead>
      <tbody>
        <% @profiles.each do |profile| %>
          <tr>
            <td><%= profile.email %></td>
            <td><%= profile.state.name %></td>
            <td><%= profile.job_title %></td>
            <td><%= profile.age %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

  <div class="panel-footer">
    <%= paginate @profiles %>
  </div>
</div>


